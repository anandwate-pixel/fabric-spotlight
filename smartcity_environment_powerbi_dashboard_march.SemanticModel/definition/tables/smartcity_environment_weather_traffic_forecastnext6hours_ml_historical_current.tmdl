	column CurrentDateHour = ```
			
			VAR raw = smartcity_environment_weather_traffic_forecastnext6hours_ml_historical_current[current_time]   // text like "2026-02-17 13:00:00+05:30"
			VAR dt =
			    DATEVALUE( LEFT(raw, 10) ) +
			    TIMEVALUE( MID(raw, 12, 8) )
			RETURN
			    DATE(YEAR(dt), MONTH(dt), DAY(dt)) + TIME(HOUR(dt), 0, 0)
			
